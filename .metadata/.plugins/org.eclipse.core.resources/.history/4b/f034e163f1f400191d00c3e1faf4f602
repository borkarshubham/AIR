import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;


class Block
{
	int id,on;
	
	public Block(int i)
	{
		this.on = -1;
		this.id = i;
	}
	
	public int getOn()
	{
		return this.on;
	}
	
	public void setOn(int i)
	{
		this.on = i;
	}
}

public class demo 
{
	List<Block> curr;
	List<Block> goal;
	boolean hand;
	int n;
	
	public demo(int n)
	{
		curr = new ArrayList<>();
		goal = new ArrayList<>();
		hand = true;
		this.n = n;
	}
	
	public static void main(System[] args)
	{
		System.out.print("Enter the number of blocks :- ");
		Scanner sc =  new Scanner(System.in);
		int n = sc.nextInt();
		
		System.out.println();
		
		
		
		
	}
}
